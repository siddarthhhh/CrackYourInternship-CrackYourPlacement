class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int k) {
        unordered_map<int,int>mpp;
        for(int i=0;i<nums.size();i++){
            if(mpp.find(k-nums[i]) != mpp.end()){
                return {mpp[k-nums[i]],i};
            }
            mpp[nums[i]]=i;
        }
        return {-1,-1};

    }
};
